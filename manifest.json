{
  "manifest_version": 3,
  "name": "Inject Tailwind CSS",
  "version": "1.0",
  "description": "Inject Tailwind CSS into the current page",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  }
}
